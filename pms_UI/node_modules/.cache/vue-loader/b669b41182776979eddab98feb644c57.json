eights\">\r\n          <el-input v-model=\"form.weights\" placeholder=\"请输入权重\"  maxlength=\"50\" show-word-limit  />\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button type=\"primary\" @click=\"submitForm\" :loading=\"saveLoading\">确 定</el-button>\r\n        <el-button @click=\"cancel\">取 消</el-button>\r\n      </div>\r\n    </el-dialog>\r\n</template>\r\n\r\n<script>\r\nimport { validEmail } from'@/utils/validate'\r\nimport { listDistributionDetail, getDistributionDetail, delDistributionDetail, addDistributionDetail, updateDistributionDetail, exportDistributionDetail } from \"@/api/second/distributionDetail\";\r\n\r\nexport default {\r\n  name: \"addEdit\",\r\n  props: {\r\n    open: {\r\n      type: Boolean,\r\n      default: false\r\n    },\r\n    id: {\r\n      type: [String, Number],\r\n      default: \"\"\r\n    }\r\n  },\r\n  watch: {\r\n    id: {\r\n      handler(val) {\r\n        this.reset();\r\n        if (val) {\r\n          this.id = val;\r\n          this.title = \"二次分配指标集明细管理-编辑\";\r\n          this.getDetailInfo();\r\n        } else {\r\n          this.id = \"\";\r\n          this.title = \"二次分配指标集明细管理-新增\";\r\n        }\r\n      },\r\n      deep: true,\r\n      immediate: true\r\n    },\r\n    open: {\r\n      handler(val) {\r\n        this.openDialog = val;\r\n      },\r\n      deep: true\r\n    }\r\n  },\r\n  components: {\r\n  },\r\n  data() {\r\n    return {\r\n      title: \"二次分配指标集明细管理-新增\",\r\n      openDialog: this.open,\r\n      saveLoading: false,\r\n      // 表单参数\r\n      form: {},\r\n      \t  \t  \t  \t  \t  \t  \t  \t  \t  \t  \t  \t  \t  \t  \t  \t  \t  \t  \t  \t        // 表单校验\r\n      rules: {\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.getList();\r\n  },\r\n  methods: {\r\n  \tgetDetailInfo() {\r\n      getDistributionDetail(this.id).then(response => {\r\n        console.log(response);\r\n        this.form = response.data;\r\n      });\r\n    },\r\n    /** 查询二次分配指标集明细列表 */\r\n    getList() {\r\n      this.loading = true;\r\n      listDistributionDetail(this.queryParams).then(response => {\r\n        this.distributionDetailList = response.rows;\r\n        this.total = response.total;\r\n        this.loading = false;\r\n      });\r\n    },\r\n    // 弹框关闭\r\n    handleDialogClose() {\r\n      this.$emit(\"cancel\", false);\r\n    },\r\n    // 取消\r\n    cancel() {\r\n      this.$emit(\"cancel\", false);\r\n    },\r\n    // 表单重置\r\n    reset() {\r\n      this.form = {\r\n        id: null,\r\n        indexCode: null,\r\n        indexName: null,\r\n        weights: null,\r\n        delFlag: null,\r\n        createBy: null,\r\n        createTime: null,\r\n        updateBy: null,\r\n        updateTime: null,\r\n        remark: null\r\n      };\r\n      this.resetForm(\"form\");\r\n    },\r\n    /** 提交按钮 */\r\n    submitForm() {\r\n      this.$refs[\"form\"].validate(valid => {\r\n        if (valid) {\r\n        this.saveLoading = true;\r\n          if (this.form.id != null) {\r\n            updateDistributionDetail(this.form).then(response => {\r\n              this.msgSuccess(\"修改成功\");\r\n              this.$emit(\"success\", false);\r\n              this.saveLoading = false;\r\n            });\r\n          } else {\r\n            addDistributionDetail(this.form).then(response => {\r\n              this.msgSuccess(\"新增成功\");\r\n              this.$emit(\"success\", false);\r\n              this.saveLoading = false;\r\n            });\r\n          }\r\n        }\r\n      });\r\n    },\r\n}};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n  ::v-deep .el-dialog {\r\n    height: 80%;\r\n    overflow: hidden;\r\n    .el-dialog__body {\r\n      height: calc(100% - 100px);\r\n      overflow: auto;\r\n    }\r\n  }\r\n</style>"]}]}