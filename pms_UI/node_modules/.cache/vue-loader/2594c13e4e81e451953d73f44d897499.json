.openDialog = val;\r\n      },\r\n      deep: true\r\n    }\r\n  },\r\n  components: {\r\n  },\r\n  data() {\r\n    return {\r\n      title: \"术式套包字典管理明细管理-新增\",\r\n      openDialog: this.open,\r\n      saveLoading: false,\r\n      // 表单参数\r\n      form: {},\r\n      \t  \t  \t  \t  \t  \t  \t  \t  \t  \t  \t  \t  \t  \t  \t  \t  \t  \t  \t  \t  \t  \t        // 表单校验\r\n      rules: {\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.getList();\r\n  },\r\n  methods: {\r\n  \tgetDetailInfo() {\r\n      getPackageDetail(this.id).then(response => {\r\n        console.log(response);\r\n        this.form = response.data;\r\n      });\r\n    },\r\n    /** 查询术式套包字典管理明细列表 */\r\n    getList() {\r\n      this.loading = true;\r\n      listPackageDetail(this.queryParams).then(response => {\r\n        this.packageDetailList = response.rows;\r\n        this.total = response.total;\r\n        this.loading = false;\r\n      });\r\n    },\r\n    // 弹框关闭\r\n    handleDialogClose() {\r\n      this.$emit(\"cancel\", false);\r\n    },\r\n    // 取消\r\n    cancel() {\r\n      this.$emit(\"cancel\", false);\r\n    },\r\n    // 表单重置\r\n    reset() {\r\n      this.form = {\r\n        id: null,\r\n        compCode: null,\r\n        packageCode: null,\r\n        materialCode: null,\r\n        quantity: null,\r\n        delFlag: null,\r\n        createBy: null,\r\n        createTime: null,\r\n        updateBy: null,\r\n        updateTime: null,\r\n        remark: null\r\n      };\r\n      this.resetForm(\"form\");\r\n    },\r\n    /** 提交按钮 */\r\n    submitForm() {\r\n      this.$refs[\"form\"].validate(valid => {\r\n        if (valid) {\r\n        this.saveLoading = true;\r\n          if (this.form.id != null) {\r\n            updatePackageDetail(this.form).then(response => {\r\n              this.msgSuccess(\"修改成功\");\r\n              this.$emit(\"success\", false);\r\n              this.saveLoading = false;\r\n            });\r\n          } else {\r\n            addPackageDetail(this.form).then(response => {\r\n              this.msgSuccess(\"新增成功\");\r\n              this.$emit(\"success\", false);\r\n              this.saveLoading = false;\r\n            });\r\n          }\r\n        }\r\n      });\r\n    },\r\n}};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n  ::v-deep .el-dialog {\r\n    height: 80%;\r\n    overflow: hidden;\r\n    .el-dialog__body {\r\n      height: calc(100% - 100px);\r\n      overflow: auto;\r\n    }\r\n  }\r\n</style>"]}]}